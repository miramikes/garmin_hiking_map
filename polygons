# City

amenity=kindergarten 										[0x0a resolution 22]
amenity=college 										[0x0a resolution 22]
amenity=school 											[0x0a resolution 22]
amenity=university 										[0x0a resolution 22]

parking=underground | parking=multi-storey 							[0x06 resolution 22]
amenity=parking | parking=surface 								[0x05 resolution 22]

healthcare=hospital | amenity=hospital 								[0x0b resolution 22]
healthcare=* | amenity=dentist | amenity=doctors | amenity=nursing_home 			[0x0b resolution 23]

leisure=common 											[0x17 resolution 22]
leisure=garden 											[0x17 resolution 22]
leisure=park 											[0x17 resolution 22]
leisure=playground 										[0x17 resolution 22]

leisure=golf_course 										[0x18 resolution 22]

leisure=ice_rink 										[0x19 resolution 22]
leisure=pitch { name '${name} (${sport})' | '${name}' } 					[0x19 resolution 22]
leisure=recreation_ground 									[0x19 resolution 22]
leisure=sports_center | leisure=sports_centre { name '${name} (${sport})' | '${name}' } 	[0x19 resolution 22]
leisure=stadium { name '${name} (${sport})' | '${name}' } 					[0x19 resolution 22]
leisure=track & area=yes { name '${name} (${sport})' | '${name}' } 				[0x19 resolution 22]

leisure=water_park 										[0x09 resolution 22]
leisure=marina 											[0x09 resolution 22]

# Shopping

amenity=supermarket | shop=supermarket | landuse=retail						[0x08 resolution 22]
amenity=cafe | amenity=fast_food | amenity=restaurant | amenity=food_court 			[0x08 resolution 22]
shop=* 		& shop!=supermarket								[0x08 resolution 22]

# squares and plazas
highway=pedestrian & area=yes 			[0x17 resolution 22]

# other highways that have area=yes set must be parking lots
highway=* & area=yes 				[0x05 resolution 22]

landuse=allotments 						[0x27 resolution 21]
landuse=cemetery | landuse=cemetary | amenity=grave_yard 	[0x1a resolution 21]
landuse=commercial 						[0x21 resolution 21]
landuse=construction 						[0x0c resolution 21]
landuse=quarry 							[0x20 resolution 21]
landuse=industrial 						[0x0c resolution 22]
landuse=residential 						[0x10 resolution 22]
landuse=retail 							[0x08 resolution 22]

landuse=recreation_ground 					[0x19 resolution 21]
landuse=greenfield 						[0x17 resolution 21]
landuse=village_green 						[0x17 resolution 21]

# building tag should be last
(building=* | amenity=*) & area!=no 		[0x13 resolution 24]
tourism=* & area!=no & waterway!=* 		[0x13 resolution 24]
man_made=* & area=yes 				[0x13 resolution 24]
# man_made can be used on areas or lines
man_made=* & area!=no & (man_made!=door & man_made!=embankment & man_made!=breakwater & man_made!=cable_line & man_made!=cutline & man_made!=cutting & man_made!=levee & man_made!=trench) 		[0x13 resolution 24]

# Historic

historic=museum | historic=memorial [0x1e resolution 21]
historic=archaeological_site | historic=ruins 	[0x1e resolution 21]

# Agriculture

landuse=farm 									[0x4e resolution 21]
landuse=farmyard 								[0x4e resolution 22]
landuse=vineyard 								[0x63 resolution 21]
natural=orchard | landuse=orchard | landuse=orchards | natural=orchards		[0x10f14 resolution 22]

# Danger

landuse=military 							[0x04 resolution 21]
military=airfield 							[0x04 resolution 21]
military=barracks 							[0x04 resolution 23]
amenity=prison 								[0x04 resolution 22]

( military=danger_area | military=range ) & landuse!=military 		[0x04 resolution 21]

