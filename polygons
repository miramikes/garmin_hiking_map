#  Nature

##############################

leisure=nature_reserve 										[0x16 resolution 20]

# Sand

natural=beach | natural=sand						[0x1100d resolution 24-21]
surface=sand & area=yes							[0x1100d resolution 24-22]
surface=sand & highway!=* 						[0x1100d resolution 24-22]
water=tidal								[0x1101b resolution 24-21]
natural=shelf | tidal=yes | tidal=true					[0x1101b resolution 24-21]

# Water

natural=bay [0x3d resolution 18]
natural=water [0x3a resolution 18]
waterway=riverbank [0x3e resolution 20]

landuse=basin | landuse=pond				[0x3e resolution 20]
landuse=reservoir 					[0x3e resolution 19]

# Vegetation

wood=mixed						[0x1101f resolution 24-21]
wood=deciduos | wood=deciduous				[0x1101e resolution 24-21]
wood=coniferous 					[0x1101d resolution 24-21]

( landuse=forest | natural=wood | landuse=wood | natural=forest )	& wood!=*	[0x50 resolution 24-21]

natural=bare_rock 		{ name '${name} rock' | 'bare_rock' }							[0x1100e resolution 22]
natural=cliff & area=yes	{ name '${name} cliff' | 'cliff' } 							[0x1100e resolution 22]

natural=scrub | natural=shrub | natural=scrubland | natural=scrubs | natural=shrubs | natural=shrubland				[0x4f resolution 24-21]
natural=scree										[0x11016 resolution 24-21]
natural=bedrock 									[0x1100f resolution 24-21]
natural=fell 										[0x11015 resolution 24-21]
natural=heath 										[0x11014 resolution 24-21]
natural=mud | natural=marsh | natural=wetland 						[0x51 resolution 24-21]
natural=meadow | landuse=meadow								[0x33 resolution 21]

natural=glacier 							[0x4d resolution 24-18]

natural=sea 								[0x10f1d resolution 24-14]
natural=land 								[0x10101 resolution 24-14 continue]

place=island & name=* 		[0x10100 resolution 20]
place=islet & name=* 		[0x10100 resolution 22]

